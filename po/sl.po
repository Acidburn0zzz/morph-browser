#: src/app/InvalidCertificateErrorSheet.qml:97
#, qt-format
msgid ""
"Subject:\n"
"%1"
msgstr ""
"Zadeva:\n"
"%1"

#. TRANSLATORS: %1 refers to the SSL certificate's subject's address
#: src/app/InvalidCertificateErrorSheet.qml:105
#, qt-format
msgid ""
"Subject address:\n"
"%1"
msgstr ""
"Naslov zadeve:\n"
"%1"

