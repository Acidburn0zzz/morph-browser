#, qt-format
msgid ""
"Issuer address:\n"
"%1"
msgstr ""
"Seòladh an fhoillsicheir:\n"
"%1"

#. TRANSLATORS: %1 refers to the SSL certificate's start date
#: src/app/InvalidCertificateErrorSheet.qml:132
#, qt-format
msgid ""
"Valid from:\n"
"%1"
msgstr ""
"Dligheach o:\n"
"%1"

