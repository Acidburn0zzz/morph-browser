{
    "name": "com.ubports.browserng",
    "description": "Next WebBrowser using QtWebEngine",
    "framework": "ubuntu-sdk-16.04",
    "architecture": "@CLICK_ARCH@",
    "title": "WebBrowser Next",
    "icon": "@CMAKE_INSTALL_DATADIR@/webbrowser-ng/webbrowser-ng.png",
    "hooks": {
        "clock": {
            "apparmor": "webbrowser-ng.apparmor",
            "desktop": "@CMAKE_INSTALL_DATADIR@/applications/webbrowser-ng.desktop",
            "urls": "@CMAKE_INSTALL_DATADIR@/url-dispatcher/urls/webbrowser-ng.url-dispatcher"
        }
    },
    "version": "0.1",
    "maintainer": "UBports Developers"
}
